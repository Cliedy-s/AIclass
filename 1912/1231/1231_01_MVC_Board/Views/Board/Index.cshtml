@model IEnumerable<_1231_01_MVC_Board.Models.Board>

@{
    ViewBag.Title = "글 목록";
}

<h2>글 목록</h2>

<p>
    @Html.ActionLink("글쓰기", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SEQ)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Subject)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.WriteTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ReadCount)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SEQ)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Subject)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WriteTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ReadCount)
            </td>
            <td>
                @Html.ActionLink("수정", "Edit", new { id = item.SEQ }) |
                @Html.ActionLink("보기", "Details", new { id = item.SEQ }) |
                @Html.ActionLink("삭제", "Delete", new { id = item.SEQ })
            </td>
        </tr>
    }

</table>
