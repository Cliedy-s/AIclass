@model Test_GVLoading.Models.ParkedIndexViewModel
@{
    ViewBag.Title = "Cart";
}
<!-- /.row -->
<div class="row">
    <div class="col-xs-12">
        <div class="box">
            <div class="box-header">
                <h2>주차 목록</h2>
            </div>
            <!-- /.box-header -->
            <div class="box-body table-responsive">
                <table class="table table-hover">
                    <tr>
                        <th>대차 코드</th>
                        <th class="text-center">종류</th>
                        <th class="text-right">적재</th>
                        <th class="text-right">용량</th>
                        <th> </th>
                    </tr>
                    @foreach (var item in Model.Parking.Parked)
                    {
                        <tr>
                            <td class="text-left">@item.GVCode</td>
                            <td class="text-center">@item.CategoryName</td>
                            <td class="text-right">@item.LoadCnt</td>
                            <td class="text-right">@item.Capacity</td>
                            <td class="text-center">
                                @using (Html.BeginForm("Out", "Parking"))
                                {
                                    @Html.Hidden("GVCode", item.GVCode)
                                    @Html.Hidden("returnUrl", Model.ReturnUrl)
                                    <input type="submit" value="빼기" class="btn btn-warning" />
                                }
                            </td>
                        </tr>
                    }
                </table>
            </div>
            <!-- /.box-body -->
        </div>
        <!-- /.box -->
        <div class="text-center">
            <a href="@Model.ReturnUrl" class="btn btn-primary">목록으로</a>
        </div>
    </div>
</div>